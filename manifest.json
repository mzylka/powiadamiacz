{
	"manifest_version": 2,
	"name": "Powiadamiacz MichasterLive",
	"description": "Powiadomienia o prowadzonych live'ach Michasterka",
	"version": "1.2.2",

	"browser_action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"64": "img/icon_64.png",
		"128": "img/icon_128.png"
	},
	"background": {
		"scripts": ["jquery.js","background.js"]
	},
	"permissions": [
		"notifications",
		"tabs"
	]
}